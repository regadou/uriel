#!/usr/bin/env uriel

service ./countries.properties create build run
put base "http://localhost:4242/"
put entities uri base "data"
each entities entity
  print "importing data for " entity " ..."
  post uri base "data" entity uri text "./" entity ".csv"
  put query data "application/x-sql" text "select count(*) from " entity
  print "records save: " post uri base "data" uri query
end each
print "all done"
print get uri base "data"

